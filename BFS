#include <stdio.h>
#define MAX 100

int adj[MAX][MAX], vis[MAX], stack[MAX], top = -1;

int main() {
    int n, start;

    printf("Enter the number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);

    printf("Enter starting vertex: ");
    scanf("%d", &start);

    printf("DFS traversal starting from vertex %d:\n", start);
    stack[++top] = start;

    while (top != -1) {
        int v = stack[top--];
        if (!vis[v]) {
            printf("%d ", v);
            vis[v] = 1;

            for (int i = n - 1; i >= 0; i--) {
                if (adj[v][i] && !vis[i]) {
                    stack[++top] = i;
                }
            }
        }
    }

    printf("\n");
    return 0;
}
